<%- include ("../../partials/header.ejs") %>
    <%- include ("../../partials/navbar.ejs") %>

        <div class="container mt-4">
            <h2>Usuários</h2>
            <a class="btn btn-success mt-4" href="/admin/users/create" role="button">Criar Novo Usuário</a>
            <table class="table table-bordered mt-4">
                <thead>
                    <tr>
                        <th>Id</th>
                        <th>Email</th>
                    </tr>
                </thead>
                <tbody>
                    <% users.forEach(user=>{ %>
                        <tr>
                            <td>
                                <%= user.id %>
                            </td>
                            <td>
                                <%= user.email %>
                            </td>
                        </tr>
                        <% }) %>
                </tbody>
            </table>
        </div>

        <%- include ("../../partials/footer.ejs") %>